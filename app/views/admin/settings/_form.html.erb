<%= semantic_form_for [:admin, @setting], :html => {:class => "form-horizontal"} do |f| %>
    <%= f.semantic_errors :var, :value%>
    <%= f.inputs do %>
        <%= f.input :var %>
        <%= f.input(:value) %>
    <% end %>
    <%= f.actions :class => 'form-group' do %>
        <div class="col-lg-offset-2 col-lg-8">
          <%= f.action :submit %>
          <%= f.action :cancel %>
        </div>
    <% end %>
<% end %>